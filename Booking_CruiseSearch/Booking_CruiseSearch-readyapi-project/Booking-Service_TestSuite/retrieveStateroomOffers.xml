  <con:testStep type="groovy" name="Fetch_RES_brand/ship/saildate/package" id="b6b8cf1b-fbbd-444f-a579-4c6261fbf5fa">
    <con:settings/>
    <con:config>
      <script>import java.io.*;
import java.util.*;
import com.eviware.soapui.model.testsuite.*;
