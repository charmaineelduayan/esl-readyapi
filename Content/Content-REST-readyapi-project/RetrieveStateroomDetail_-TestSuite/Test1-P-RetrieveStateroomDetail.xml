  AS400_Mirror.dbo.ICDCKD D On B.BADKCD = D.AQDKCD And B.BASHIP = D.AQSHIP&#13;
  Inner Join&#13;
  AS400_Mirror.dbo.ICSHPD S On B.BASHIP = S.SHSHIP Inner Join&#13;
  AS400_Mirror.dbo.ICCTCD On AS400_Mirror.dbo.ICCTCD.LKCATG = B.BACBIO&#13;
    And (AS400_Mirror.dbo.ICCTCD.LKBRND = S.SHBRND) Inner Join&#13;
  AS400_Mirror.dbo.ICCTPD On B.BASHIP = AS400_Mirror.dbo.ICCTPD.ARSHIP&#13;
    And B.BACTYP = AS400_Mirror.dbo.ICCTPD.ARCTYP&#13;
Where S.SHPBRD Like 'R' And B.BACBDD = '0' And&#13;
  B.BAEFDT = (Select Max(AS400_Mirror.dbo.ICSCBD.BAEFDT)&#13;
  From AS400_Mirror.dbo.ICSCBD&#13;
  Where AS400_Mirror.dbo.ICSCBD.BASHIP = B.BASHIP And&#13;
    AS400_Mirror.dbo.ICSCBD.BAEFDT &lt;= 1190305 And AS400_Mirror.dbo.ICSCBD.BACBNO&#13;
    = B.BACBNO)</con:query>
