  <con:testStep type="groovy" name="Fetch_REQ_app/country/currency" id="f7f02afd-d54f-437c-b22d-4b9f863e7f3c">
    <con:settings/>
    <con:config>
      <script>import java.io.*;
import java.util.*;
import com.eviware.soapui.model.testsuite.*;
import com.eviware.soapui.impl.wsdl.teststeps.assertions.*;
