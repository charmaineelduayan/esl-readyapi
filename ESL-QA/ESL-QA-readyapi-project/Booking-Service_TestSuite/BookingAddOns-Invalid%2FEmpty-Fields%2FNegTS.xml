import groovy.util.*

def ts = testRunner.testCase.getTestStepByName("retrieveBooking");
def retxml = new XmlSlurper().parseText(ts.getPropertyValue("ResponseAsXml"));

def ts2 = testRunner.testCase.getTestStepByName("updateBooking-firstName,address");
def updxml2 = new XmlSlurper().parseText(ts2.getPropertyValue("ResponseAsXml"));

//Get FirstName in retrieveBooking
def retFirstName  = retxml.booking.guests.guest[0].e.name.firstName[0].text()
log.info('First Name before Update: ' + retFirstName)

//Get FirstName in updateBooking
def updFirstName  = updxml2.booking.guests.guest[0].e.name.firstName[0].text()
log.info('First Name after Update: ' + updFirstName)

assert retFirstName != updFirstName
log.info('First Name is now updated from ' + retFirstName + ' to ' + updFirstName + ' ')


//Get Address of 1st Guest in retrieveBooking
def retAddress = retxml.booking.guests.guest[0].e.addresses.address.e.address1.text()
log.info('Address before Update: ' + retAddress)

//Get Address of 2nd Guest in UpdateBooking
def updAddress = updxml2.booking.guests.guest[0].e.addresses.address.e.address1.text()
log.info('Address after Update: ' + updAddress)

assert retAddress != updAddress
log.info('Address1 is now updated from ' + retAddress + ' to ' + updAddress + ' ')</script>
