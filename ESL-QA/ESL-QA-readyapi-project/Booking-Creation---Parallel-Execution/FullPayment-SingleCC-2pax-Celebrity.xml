  <con:testStep type="groovy" name="Fetch_RES_brand/ship/saildate/package" id="f5b0dbe8-cc4c-486d-9ed7-1344d98340b2">
    <con:settings/>
    <con:config>
      <script>import java.io.*;
import java.util.*;
import com.eviware.soapui.model.testsuite.*;
